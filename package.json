{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=3000",
    "build": "tsc --build --clean && vite build",
    "start": "vite serve --host --port 3505",
    "setup": "npm i && npm run build && pm2 start --name portfolio npm -- start --update-env --time && pm2 save",
    "deploy": "git pull --no-rebase && npm i && npm run build && pm2 restart portfolio --update-env --time && pm2 save",
    "cp": "node cli/cp.mjs"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@react-three/drei": "^9.112.0",
    "@react-three/fiber": "^8.17.6",
    "axios": "^1.7.7",
    "framer-motion": "^11.5.3",
    "maath": "^0.10.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-parallax-tilt": "^1.7.237",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-tilt": "^1.0.2",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.168.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
